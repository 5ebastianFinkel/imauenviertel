#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/sfinkel/dev/imauenviertel/node_modules/.pnpm/@mapbox+mapbox-gl-style-spec@13.28.0/node_modules/@mapbox/mapbox-gl-style-spec/bin/node_modules:/Users/sfinkel/dev/imauenviertel/node_modules/.pnpm/@mapbox+mapbox-gl-style-spec@13.28.0/node_modules/@mapbox/mapbox-gl-style-spec/node_modules:/Users/sfinkel/dev/imauenviertel/node_modules/.pnpm/@mapbox+mapbox-gl-style-spec@13.28.0/node_modules/@mapbox/node_modules:/Users/sfinkel/dev/imauenviertel/node_modules/.pnpm/@mapbox+mapbox-gl-style-spec@13.28.0/node_modules:/Users/sfinkel/dev/imauenviertel/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/sfinkel/dev/imauenviertel/node_modules/.pnpm/@mapbox+mapbox-gl-style-spec@13.28.0/node_modules/@mapbox/mapbox-gl-style-spec/bin/node_modules:/Users/sfinkel/dev/imauenviertel/node_modules/.pnpm/@mapbox+mapbox-gl-style-spec@13.28.0/node_modules/@mapbox/mapbox-gl-style-spec/node_modules:/Users/sfinkel/dev/imauenviertel/node_modules/.pnpm/@mapbox+mapbox-gl-style-spec@13.28.0/node_modules/@mapbox/node_modules:/Users/sfinkel/dev/imauenviertel/node_modules/.pnpm/@mapbox+mapbox-gl-style-spec@13.28.0/node_modules:/Users/sfinkel/dev/imauenviertel/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/gl-style-format.js" "$@"
else
  exec node  "$basedir/../../bin/gl-style-format.js" "$@"
fi
