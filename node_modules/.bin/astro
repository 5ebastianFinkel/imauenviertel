#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/sfinkel/dev/imauenviertel/node_modules/.pnpm/astro@5.11.0_@types+node@24.0.0_jiti@2.4.2_lightningcss@1.30.1_rollup@4.43.0_typescript@5.8.3_yaml@2.8.0/node_modules/astro/node_modules:/Users/sfinkel/dev/imauenviertel/node_modules/.pnpm/astro@5.11.0_@types+node@24.0.0_jiti@2.4.2_lightningcss@1.30.1_rollup@4.43.0_typescript@5.8.3_yaml@2.8.0/node_modules:/Users/sfinkel/dev/imauenviertel/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/sfinkel/dev/imauenviertel/node_modules/.pnpm/astro@5.11.0_@types+node@24.0.0_jiti@2.4.2_lightningcss@1.30.1_rollup@4.43.0_typescript@5.8.3_yaml@2.8.0/node_modules/astro/node_modules:/Users/sfinkel/dev/imauenviertel/node_modules/.pnpm/astro@5.11.0_@types+node@24.0.0_jiti@2.4.2_lightningcss@1.30.1_rollup@4.43.0_typescript@5.8.3_yaml@2.8.0/node_modules:/Users/sfinkel/dev/imauenviertel/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/astro@5.11.0_@types+node@24.0.0_jiti@2.4.2_lightningcss@1.30.1_rollup@4.43.0_typescript@5.8.3_yaml@2.8.0/node_modules/astro/astro.js" "$@"
else
  exec node  "$basedir/../.pnpm/astro@5.11.0_@types+node@24.0.0_jiti@2.4.2_lightningcss@1.30.1_rollup@4.43.0_typescript@5.8.3_yaml@2.8.0/node_modules/astro/astro.js" "$@"
fi
